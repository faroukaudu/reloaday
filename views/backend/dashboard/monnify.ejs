    <script>
        $(document).ready(function () {
            // Listen for input event on the form input field
            $('.amount').on('input', function () {
                // Get the current value of the input field
                const inputValue = $(this).val();
                const amtC = Number(inputValue);
                const total = amtC;

                // Update the text in the display div
                $('.perc').text("₦" + total.toLocaleString());
            });
        });


    </script>

